<aura:component implements="lightning:homeTemplate" description="This custom home page tamplate for Personnel Management lightning app">
	<aura:attribute name="firstElement" type="Aura.Component[]" />
    <aura:attribute name="secondElement" type="Aura.Component[]" />
    
    <div>
        <lightning:layout horizontalAlign="spread">
            <lightning:layoutItem flexibility="grow" padding="slds-col"  size="{! $Browser.isDesktop ? '12' : '190' }" > 
                {!v.firstElement}
            </lightning:layoutItem>
            
        </lightning:layout>
    </div>
    <div>
        <lightning:layout horizontalAlign="spread">
            <lightning:layoutItem flexibility="grow" padding="slds-col"  size="{! $Browser.isDesktop ? '12' : '190' }" > 
                {!v.secondElement}
            </lightning:layoutItem>
            
        </lightning:layout>
    </div>
</aura:component>