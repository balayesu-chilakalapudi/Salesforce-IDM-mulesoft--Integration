<aura:component controller="DPM_PersonnelMasterController"> 
    <aura:attribute name="w9MaxDate" type="String" description="Stores the Max Date"/> 
    <aura:attribute name="IsSupplier" type="Boolean" default="false" description="Checks if its to create supplier Employee"/> 
    <aura:attribute name="mode" type="String" description="Used to describe the mode - view/edit/admin" default="view"/> 
    <aura:attribute name="personRole" type="Map" description="Used to store the Person Role for displaying the User Profile"/> 
    <aura:attribute name="genderList" type="List" description="Stores the Gender Picklist values" default="[]"/> 
    <aura:attribute name="prefixList" type="List" description="Stores the Prefix Picklist values" default="[]"/> 
    <aura:attribute name="w9flagList" type="List" description="Stores the W9-Flag Picklist values" default="[]"/> 
    <aura:attribute name="suffixList" type="List" description="Stores the Suffix Picklist values " /> 
    <aura:attribute name="incentivePositionsList" type="List" description="Stores the Incentive Eligible Positions"/> 
    <aura:attribute name="ownerPositionsList" type="List" description="Stores the Owner Positions"/> 
    <aura:attribute name="primaryPositionsList" type="List" description="Stores the Primary Positions for the DPM Employee"/> 
    <aura:attribute name="incentiveEligiblePositionsList" type="List" description="Stores the Incentive Eligible Positions for the DPM Employee"/> 
    <aura:attribute name="positionsList" type="List" description="Stores the Positions Picklist values for the DPM Employee"/> 
    <aura:attribute name="countryList" type="List" description="Stores the Country Picklist values" default="[]"/> 
    <aura:attribute name="stateList" type="List" description="Stores the State Picklist values" default="[]"/> 
    <aura:attribute name="stateListWork" type="List" description="Stores the State Picklist values for Work Address" default="[]"/> 
    <aura:attribute name="mapCountryWithStates" type="Map" description="Stores the map of country with states"/> 
    <aura:attribute name="showSSN" type="Boolean" default="false" description="Used for showing SSN"/> 
    <aura:attribute name="SSNBtnLabel" type="String" default="" description="Used for showing SSN button label"/> 
    <aura:attribute name="isRetailerAdmin" type="boolean" default="false" description="Used to determine whether the current logged in User is Retailer Admin"/> 
    <aura:attribute name="IsShowSSNSupplier" type="boolean" default="true" description="Used to determine whether the current logged in User is Supplier Emp"/> 
    <aura:attribute name="positionEditable" type="boolean" default="false" description="Used to determine whether the positions can be edited"/> 
    <aura:attribute name="homeAddressEditable" type="boolean" default="false" description="Used to determine whether the home address can be edited"/> 
    <aura:attribute name="isSearchModal" type="boolean" default="false" description="Used to determine a search modal"/> 
    <aura:attribute name="searchMode" type="String" default="" description="Used to determine the search mode"/> 
    <aura:attribute name="corporatePermissions" default="{}" type="Map" description="Used to store corporate permissions"/> 
    <aura:attribute name="DPMProficiency" type="List" description="Stores the DPMProficiency1 Picklist values " /> 
    <aura:attribute name="languageList" type="List" description="Stores the Language Picklist values" default=""/> 
    <aura:attribute name="raceList" type="List" description="Stores the Race Picklist values" default=""/> 
    <aura:attribute name="otherRace" type="String" description="Stores the other Race value" default=""/> 
    <aura:attribute name="otherEthnicity" type="String" description="Stores the other Ethnicity value" default=""/> 
    <aura:attribute name="ethnicityList" type="List" description="Stores the Ethnicity Picklist values" default=""/> 
    <aura:attribute name="diversityData" type="Object" description="The map of all the diversity data by country" /> 
    <aura:attribute name="blnEditDMSId" type="boolean" default="false" description="Used to give edit access to DMS Id" /> 
    <aura:attribute name="account" type="Object" description="Store the current dealer store account" /> 
    <aura:attribute name="fromPortal" type="boolean" default="false" description="Used to identify portal type" /> 
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/> 
    <aura:handler name="change" value="{!v.mode}" action="{!c.modeChangeHandler}"/> 
    <aura:handler name="change" value="{!v.personRole.RE_Contact__r.MailingCountry}" action="{!c.countryChangeHandler}"/> 
    <aura:handler name="dpmNotificationEvt" event="c:DPM_NotificationModalEvent" action="{!c.handleNotificationEvent}"/> 
    <aura:handler name="languageEvent" event="c:DPM_LanguageEvent" action="{!c.handleLanguageVal}"/> 
    <aura:registerEvent name="dpmTerminationDateMessagesEvt" type="c:DPM_TerminationDateMessagesEvent" /> 
    <aura:registerEvent name="dpmCloseEditEmployeeEvt" type="c:DPM_CloseEditEmployeeEvent"               />
    
    <div class="{!v.mode=='view'?'slds-box':''}">   
        {!v.body} 
        <lightning:spinner aura:id="idSpinner" alternativeText="Loading" size="medium" variant="brand" class="{!v.mode=='view'?'slds-hide':''}"></lightning:spinner> 
        <lightning:accordion allowMultipleSectionsOpen="true" activeSectionName="Main Information"> 
            <lightning:accordionSection name="Main Information" label="Main Information"> 
                <div class="{!v.mode=='view'?'slds-grid slds-wrap slds-m-left_medium':'slds-grid slds-wrap slds-m-left_small'}">                 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"> 
                        <div class="slds-form-element"> 
                            <aura:if isTrue="{!v.mode=='view'}"> 
                                <span class="slds-form-element__label">Prefix</span> 
                            </aura:if>                     
                            <div class="slds-form-element__control classFormElement"> 
                                <div class="slds-form-element__static"> 
                                    <aura:if isTrue="{!v.mode=='view'}"> 
                                        {!v.personRole.RE_Contact__r.Salutation} 
                                        <aura:set attribute="else"> 
                                            <lightning:select disabled ="{!v.corporatePermissions.viewW9}" aura:id="idPrefix" label="Prefix" value="{!v.personRole.RE_Contact__r.Salutation}" required="true" messageWhenValueMissing="Select Prefix"> 
                                                <aura:iteration items="{!v.prefixList}" var="item"> 
                                                    <option text="{!item.text}" value="{!item.value}" selected="{!item.value==v.personRole.RE_Contact__r.Salutation}"></option> 
                                                </aura:iteration> 
                                            </lightning:select> 
                                        </aura:set> 
                                    </aura:if> 
                                </div> 
                            </div> 
                        </div> 
                    </div> 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"> 
                        <div class="slds-form-element"> 
                            <aura:if isTrue="{!v.mode=='view'}"> 
                                <span class="slds-form-element__label">Legal First Name</span> 
                            </aura:if> 
                            <div class="slds-form-element__control classFormElement"> 
                                <form class="slds-form-element__static" autocomplete="off"> 
                                    <aura:if isTrue="{!v.mode=='view'}"> 
                                        {!v.personRole.RE_Contact__r.FirstName} 
                                        <aura:set attribute="else"> 
                                            <lightning:input aura:id="idFirstName" required="true" label="Legal First Name" type="text" value="{!v.personRole.RE_Contact__r.FirstName}" maxlength="255" 
                                                             messageWhenValueMissing="Enter Legal First Name" 
                                                             disabled ="{!v.corporatePermissions.viewW9}"/> 
                                        </aura:set> 
                                    </aura:if>                             
                                </form> 
                            </div> 
                        </div> 
                    </div> 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"> 
                        <div class="slds-form-element"> 
                            <aura:if isTrue="{!v.mode=='view'}"> 
                                <span class="slds-form-element__label">Last Name</span> 
                            </aura:if> 
                            <div class="slds-form-element__control classFormElement"> 
                                <form class="slds-form-element__static" autocomplete="off"> 
                                    <aura:if isTrue="{!v.mode=='view'}"> 
                                        {!v.personRole.RE_Contact__r.LastName} 
                                        <aura:set attribute="else"> 
                                            <lightning:input aura:id="idLastName" required="true" label="Last Name" type="text" value="{!v.personRole.RE_Contact__r.LastName}" maxlength="255" 
                                                             disabled ="{!v.corporatePermissions.viewW9}" messageWhenValueMissing="Enter Legal Last Name" 
                                                             /> 
                                        </aura:set> 
                                    </aura:if>  
                                </form> 
                            </div> 
                        </div> 
                    </div> 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"> 
                        <div class="slds-form-element"> 
                            <aura:if isTrue="{!v.mode=='view'}"> 
                                <span class="slds-form-element__label">Middle Name</span> 
                            </aura:if> 
                            <div class="slds-form-element__control classFormElement"> 
                                <form class="slds-form-element__static" autocomplete="off"> 
                                    <aura:if isTrue="{!v.mode=='view'}"> 
                                        {!v.personRole.RE_Contact__r.MiddleName} 
                                        <aura:set attribute="else"> 
                                            <lightning:input disabled ="{!v.corporatePermissions.viewW9}" type="text" label="Middle Name" value="{!v.personRole.RE_Contact__r.MiddleName}" maxlength="255"                                                          
                                                             /> 
                                        </aura:set> 
                                    </aura:if>  
                                </form> 
                            </div> 
                        </div> 
                    </div> 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"> 
                        <div class="slds-form-element"> 
                            <aura:if isTrue="{!v.mode=='view'}"> 
                                <span class="slds-form-element__label">Suffix</span> 
                            </aura:if> 
                            <div class="slds-form-element__control classFormElement"> 
                                <div class="slds-form-element__static"> 
                                    <aura:if isTrue="{!v.mode=='view'}"> 
                                        {!v.personRole.RE_Contact__r.Suffix} 
                                        <aura:set attribute="else"> 
                                            <lightning:select disabled ="{!v.corporatePermissions.viewW9}" aura:id="idSuffix" label="Suffix" value="{!v.personRole.RE_Contact__r.Suffix}" messageWhenValueMissing="Select Prefix"> 
                                                <aura:iteration items="{!v.suffixList}" var="item"> 
                                                    <option text="{!item.text}" value="{!item.value}" selected="{!item.value==v.personRole.RE_Contact__r.Suffix}"></option> 
                                                </aura:iteration> 
                                            </lightning:select> 
                                        </aura:set> 
                                    </aura:if> 
                                </div> 
                            </div> 
                        </div> 
                    </div> 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"> 
                        <div class="slds-form-element"> 
                            <span class="slds-form-element__label">Preferred First Name</span> 
                            <div class="slds-form-element__control"> 
                                <form class="slds-form-element__static" autocomplete="off"> 
                                    <aura:if isTrue="{!v.mode=='view'}"> 
                                        {!v.personRole.RE_Contact__r.DPM_Preferred_First_Name__c} 
                                        <aura:set attribute="else"> 
                                            <lightning:input disabled ="{!v.corporatePermissions.viewW9}" variant="label-hidden" type="text" value="{!v.personRole.RE_Contact__r.DPM_Preferred_First_Name__c}" maxlength="255"               
                                                             /> 
                                        </aura:set> 
                                    </aura:if> 
                                </form> 
                            </div> 
                        </div> 
                    </div> 
                    <!-- 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':''}"/> 
                    --> 
                    <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"> 
                        <div class="slds-form-element"> 
                            <aura:if isTrue="{!v.mode=='view'}"> 
                                <span class="slds-form-element__label">Gender</span> 
                            </aura:if> 
                            <div class="slds-form-element__control classFormElement"> 
                                <div class="slds-form-element__static"> 
                                    <aura:if isTrue="{!v.mode=='view'}"> 
                                        {!v.personRole.RE_Contact__r.DPM_Gender__c}                                          
                                        <aura:set attribute="else"> 
                                            <lightning:select disabled ="{!v.corporatePermissions.viewW9}" aura:id="idGender" required="true" label="Gender" value="{!v.personRole.RE_Contact__r.DPM_Gender__c}" messageWhenValueMissing="Select Gender"> 
                                                <aura:iteration items="{!v.genderList}" var="item"> 
                                                    <option text="{!item.text}" value="{!item.value}" selected="{!item.value==v.personRole.RE_Contact__r.DPM_Gender__c}"></option> 
                                                </aura:iteration> 
                                            </lightning:select> 
                                        </aura:set> 
                                    </aura:if> 
                                </div> 
                            </div> 
                        </div> 
                    </div> 
                    <!--<div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':''}"> 
                        </div>--> 
                    <!--<aura:if isTrue="{!v.personRole.RE_Contact__r.Account_Country__c == 'MEX'}"> 
                        <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':'slds-col slds-size_1-of-1 slds-large-size_4-of-12'}"></div> 
                    </aura:if>--> 
                    <aura:if isTrue="{!v.personRole.RE_Contact__r.DPM_Gender__c=='Non-binary'}"> 
                        <div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':''}"> 
                            <aura:if isTrue="{!v.mode=='view'}"> 
                                Other Gender<br /> 
                                {!v.personRole.RE_Contact__r.DPM_Other_Gender__c} 
                                <aura:set attribute="else"> 
                                    <div style="margin-left:5%;margin-top:5%"> 
                                        <lightning:input aura:id="otherbox2"                                                           
                                                         type="text" label="Other Gender" value="{!v.personRole.RE_Contact__r.DPM_Other_Gender__c}" onblur="{!c.capitalizeInput}" maxlength="255" messageWhenValueMissing="Enter Other Gender"/> 
                                    </div> 
                                </aura:set> 
                            </aura:if>        
                        </div> 
                        <!--<div class="{!v.mode=='view'?'slds-col slds-size_1-of-1 slds-large-size_3-of-12':''}"> 
                        </div>-->
                    </aura:if> 
                    <aura:if isTrue="{!(v.personRole.RE_Contact__r.MailingCountry != 'Mexico' &amp;&amp; v.IsShowSSNSupplier)}"> 
                        <aura:if isTrue="{!!v.isSearchModal || v.corporatePermissions.viewSSN || v.corporatePermissions.viewSSNLastFour}">
                            <div class="{!v.mode=='view'?'slds-size_1-of-1 slds-large-size_12-of-12':'slds-col slds-size_1-of-1 slds-large-size_8-of-12'}"> 
                                <div class="slds-grid"> 
                                    <aura:if isTrue="{!v.personRole.RE_Contact__r.MailingCountry != 'Canada'}"> 
                                        <div class="{!v.mode=='view'?'slds-size_1-of-1 slds-large-size_3-of-12':'slds-size_1-of-1 slds-large-size_12-of-12'}"> 
                                            <aura:if isTrue="{!v.isSearchModal}"> 
                                                <div class="{!v.mode=='view'?'':'classSSNEdit'}"> 
                                                    <aura:if isTrue="{!v.mode=='view'}">                                     
                                                        <aura:if isTrue="{!v.corporatePermissions.viewSSN}"> 
                                                            <c:Volvo_SSNCmp aura:id="idSSNcmp" mode="view" SSNValue="{!v.personRole.RE_Contact__r.DPM_SSN__c}" showSSN="{!v.showSSN}" showSSNBtn="{!v.personRole.RE_Contact__r.DPM_SSN__c!=''}" SSNBtnLabel="{!v.SSNBtnLabel}" strCountry="{!v.personRole.RE_Contact__r.MailingCountry}" corporatePermissions="{!v.corporatePermissions}"/> 
                                                            <aura:set attribute="else"> 
                                                                <c:Volvo_SSNCmp aura:id="idSSNcmp" mode="view" SSNValue="{!v.personRole.RE_Contact__r.DPM_SSNLastFour}" showSSN="{!v.showSSN}" showSSNBtn="{!v.personRole.RE_Contact__r.DPM_SSN__c!=''}" SSNBtnLabel="{!v.SSNBtnLabel}" strCountry="{!v.personRole.RE_Contact__r.MailingCountry}" corporatePermissions="{!v.corporatePermissions}" /> 
                                                            </aura:set> 
                                                        </aura:if> 
                                                        <aura:set attribute="else"> 
                                                            <c:Volvo_SSNCmp aura:id="idSSNcmpEdit" mode="edit" SSNValue="{!v.personRole.RE_Contact__r.DPM_SSN__c}" showSSN="{!v.showSSN}" showSSNBtn="{!v.personRole.RE_Contact__r.DPM_SSN__c!=''}" SSNBtnLabel="{!v.SSNBtnLabel}" strCountry="{!v.personRole.RE_Contact__r.MailingCountry}" corporatePermissions="{!v.corporatePermissions}" /> 
                                                        </aura:set> 
                                                    </aura:if> 
                                                </div> 
                                                <aura:set attribute="else"> 
                                                    <!--<div class="{!v.mode=='view'?'classSSN':'classSSNEdit'}"> -->
                                                        <aura:if isTrue="{!v.isRetailerAdmin}"> 
                                                            <c:Volvo_SSNCmp aura:id="idSSNcmp" mode="{!v.mode}" SSNValue="{!v.personRole.RE_Contact__r.DPM_SSNLastFour}" showSSN="{!v.showSSN}" showSSNBtn="{!v.personRole.RE_Contact__r.DPM_SSN__c!=''}" SSNBtnLabel="{!v.SSNBtnLabel}" strCountry="{!v.personRole.RE_Contact__r.MailingCountry}" corporatePermissions="{!v.corporatePermissions}" isRetailerAdmin="{!v.isRetailerAdmin}"  /> 
                                                            <aura:set attribute="else"> 
                                                                <aura:if isTrue="{!v.personRole.RE_Contact__r.DPM_Gender__c=='Non-binary'}"> 
                                                                    <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12"> 
                                                                		<c:Volvo_SSNCmp aura:id="idSSNcmp" mode="{!v.mode}" SSNValue="{!v.personRole.RE_Contact__r.DPM_SSN__c}" showSSN="{!v.showSSN}" showSSNBtn="{!v.personRole.RE_Contact__r.DPM_SSN__c!=''}" SSNBtnLabel="{!v.SSNBtnLabel}" strCountry="{!v.personRole.RE_Contact__r.MailingCountry}" corporatePermissions="{!v.corporatePermissions}" isRetailerAdmin="{!v.isRetailerAdmin}" />
                                                                    </div>
                                                                    <aura:set attribute="else"> 
                                                                        <c:Volvo_SSNCmp aura:id="idSSNcmp" mode="{!v.mode}" SSNValue="{!v.personRole.RE_Contact__r.DPM_SSN__c}" showSSN="{!v.showSSN}" showSSNBtn="{!v.personRole.RE_Contact__r.DPM_SSN__c!=''}" SSNBtnLabel="{!v.SSNBtnLabel}" strCountry="{!v.personRole.RE_Contact__r.MailingCountry}" corporatePermissions="{!v.corporatePermissions}" isRetailerAdmin="{!v.isRetailerAdmin}"/>
                                                                    </aura:set>
                                                                </aura:if>
                                                            </aura:set> 
                                                        </aura:if> 
                                                        <!--<c:Volvo_SSNCmp mode="{!v.isRetailerAdmin?'view':v.mode}" SSNValue="{!v.personRole.RE_Contact__r.DPM_SSN__c}" showSSN="{!v.showSSN}" showSSNBtn="{!v.personRole.RE_Contact__r.DPM_SSN__c!=''}" SSNBtnLabel="{!v.SSNBtnLabel}" strCountry="{!v.personRole.RE_Contact__r.MailingCountry}"/>--> 
                                                    <!--</div>--> 
                                                </aura:set> 
                                            </aura:if> 
                                        </div> 
                                    </aura:if> 
                                    
                                    
                                </div>
                            </div> 
                        </aura:if> 
                        <!--<aura:if isTrue="{!v.mode=='view'}">-->  
                         
                    </aura:if> 
                    <aura:if isTrue="{!OR(v.personRole.RE_Contact__r.RecordType.Name == 'Supplier Employee',v.personRole.RE_Contact__r.Account_Country__c == 'MEX',v.personRole.RE_Contact__r.DPM_Gender__c=='Non-binary')}"> 
                        <div class="slds-col slds-size_1-of-1 slds-large-size_12-of-12">
                            <div class="slds-form-element">                            
                                <span class="slds-form-element__label">VPID</span>                                             
                                <div class="slds-form-element__control classFormElement"> 
                                    <div class="slds-form-element__static">                                   
                                        {!v.personRole.RE_Contact__r.PsnId__c}  
                                    </div> 
                                </div> 
                            </div> 
                        </div> 
                        <aura:set attribute="else"> 
                            <!--<aura:if isTrue="{!v.personRole.RE_Contact__r.DPM_Gender__c=='Non-binary'}"> 
                                <div class="slds-col slds-size_1-of-1 slds-large-size_2-of-12"> 
                                    <div class="slds-form-element">                            
                                        <span class="slds-form-element__label">VPID</span>                                             
                                        <div class="slds-form-element__control classFormElement"> 
                                            <div class="slds-form-element__static">                                   
                                                {!v.personRole.RE_Contact__r.PsnId__c}  
                                            </div> 
                                        </div> 
                                    </div> 
                                </div> 
                                <aura:set attribute="else">-->
                                    <div class="slds-col slds-size_1-of-1 slds-large-size_3-of-12"> 
                                        <div class="slds-form-element">                            
                                            <span class="slds-form-element__label">VPID</span>                                             
                                            <div class="slds-form-element__control classFormElement"> 
                                                <div class="slds-form-element__static">                                   
                                                    {!v.personRole.RE_Contact__r.PsnId__c}  
                                                </div> 
                                            </div> 
                                        </div> 
                                    </div> 
                                <!--</aura:set>
                            </aura:if>-->
                            
                        </aura:set>
                    </aura:if>
                    <!--</aura:if>--> 
                    <aura:if isTrue="{!v.mode=='view'}">   
                        <aura:if isTrue="{!!v.fromPortal}"> 
                            <aura:if isTrue="{!v.personRole.RE_Contact__r.RecordType.Name != 'Supplier Employee' &amp;&amp; v.personRole.RE_Contact__r.Account_Country__c != 'MEX'}"> 
                                <div class="slds-col slds-size_1-of-1 slds-large-size_3-of-12">
                                    <div class="slds-form-element">                            
                                        <span class="slds-form-element__label">DMS Employee#</span>                                             
                                        <div class="slds-form-element__control classFormElement"> 
                                            <div class="slds-form-element__static">                                   
                                                {!v.personRole.DPM_DMS_Id__c}  
                                            </div> 
                                        </div> 
                                    </div>  
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-large-size_3-of-12"> 
                                    <div class="slds-form-element">                            
                                        <span class="slds-form-element__label"></span>                                             
                                        <div class="slds-form-element__control classFormElement"> 
                                            <div class="slds-form-element__static">  
                                            </div> 
                                        </div> 
                                    </div> 
                                </div>
                                <div class="slds-col slds-size_1-of-1 slds-large-size_3-of-12"> 
                                    <div class="slds-form-element">                            
                                        <span class="slds-form-element__label"></span>                                             
                                        <div class="slds-form-element__control classFormElement"> 
                                            <div class="slds-form-element__static">  
                                            </div> 
                                        </div> 
                                    </div> 
                                </div>
                            </aura:if>  
                            <aura:set attribute="else"> 
                                <aura:if isTrue="{!v.personRole.RE_Contact__r.RecordType.Name != 'Supplier Employee' &amp;&amp; v.account.Country__r.Name != 'MEX'}">
                                    <div class="slds-col slds-size_1-of-1 slds-large-size_3-of-12">
                                        <div class="slds-form-element">                            
                                            <span class="slds-form-element__label">DMS Employee#</span>                                             
                                            <div class="slds-form-element__control classFormElement"> 
                                                <div class="slds-form-element__static">                                   
                                                    {!v.personRole.DPM_DMS_Id__c}  
                                                </div> 
                                            </div> 
                                        </div>  
                                    </div>
                                </aura:if> 
                            </aura:set> 
                        </aura:if> 
